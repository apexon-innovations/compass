{
  "tooltip": true,
  "toolTipType": "color",
  "legend": false,
  "width": "100%",
  "height": 350,
  "margin": {
    "top": 10,
    "right": 10,
    "left": 0,
    "bottom": 0
  },
  "x": {
    "axisLine": true,
    "tickLine": true,
    "stroke": "#01ffff",
    "allowDataOverflow": true,
    "dataKey": "name",
    "dx": 0,
    "angle": -90,
    "height": 80,
    "fontSize": 12,
    "tick": {
      "verticalAnchor": "start",
      "fill": "#01FFFF"
    }
  },
  "y": {
    "tick": {
      "fill": "#01ffff",
