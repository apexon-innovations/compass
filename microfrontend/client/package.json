{
  "name": "client",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@khanacademy/react-multi-select": "^0.3.3",
    "node-sass": "^8.0.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^18.2.0",
    "react-notifications": "^1.7.4",
    "react-router-dom": "^6.10.0",
    "react-scripts": "^5.0.1",
    "react-svg": "^16.1.7",
    "react-tooltip": "^5.11.1",
    "reusable-components": "file:../reusable-components"
  },
  "scripts": {
    "start": "PORT=3002 react-app-rewired start",
    "start:win": "set PORT=3002 && react-app-rewired start",
    "start:client": "(cd ../des-reusable-components && npm link && npm run start:watch) & (cd ../des-login && npm link @des-infostretch/reusable-components && npm run start) & (cd ../des-client && npm link @des-infostretch/reusable-components && npm run start)",
    "build": "react-app-rewired build",
    "test": "react-scripts test",
    "test-report": "react-scripts test --coverage --watchAll -u",
    "eject": "react-scripts eject",
    "analyze": "node analyze.js",
    "pretty": "prettier --write {src}/**/*.js",
    "lint": "eslint --fix"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "^.+\\.module\\.(css|sass|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "^.+\\.(css|sass|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "^@/components/(.*)$": "<rootDir>/components/$1"
    },
    "collectCoverageFrom": [
      "src/**/*.js",
      "!<rootDir>/node_modules/",
      "!**/__tests__/**"
    ],
    "testMatch": [
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 90,
        "statements": 90
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "babel-jest": "^29.5.0",
    "bootstrap": "^5.2.3",
    "eslint": "^8.37.0",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.7",
    "progress-bar-webpack-plugin": "^2.1.0",
    "react-app-rewire-aliases": "^0.2.0",
    "react-app-rewired": "^2.2.1",
    "react-bootstrap": "^2.7.4",
    "webpack-bundle-analyzer": "^4.8.0"
  }
}
