{
  "tooltip": true,
  "toolTipType": "color",
  "legend": false,
  "width": "100%",
  "height": 300,
  "margin": {
    "top": 10,
    "right": 10,
    "left": 0,
    "bottom": 0
  },
  "x": {
    "axisLine": true,
    "tickLine": true,
    "stroke": "#01ffff",
    "allowDataOverflow": true,
    "dataKey": "name",
    "dx": 0,
    "angle": -90,
    "height": 80,
    "tick": {
      "verticalAnchor": "middle",
      "fill": "#01ffff"
    }
  },
  "y": {
    "tick": {
      "fill": "#01ffff"
    },
    "stroke": "#01ffff",
    "axisLine": true,
    "tickLine": true,
    "label": "Points #",
    "dy": 150,
    "angle": -90,
    "fontSize": 13,
    "fill": "#01ffff",
    "interval": 0,
    "domain": [
      0,
      "dataMax + 100"
    ]
  },
  "cartesianGrid": {
    "horizontal": true,
    "vertical": false,
    "stroke": "#01ffff",
    "fillOpacity": 0.5,
    "fill": true
  },
  "line": [
    {
      "key": "StoryPlanned.Backlog",
      "dot": {
        "fill": "#34cb2e"
      },
      "stroke": "#34cb2e"
    },
    {
      "key": "StoryPlanned.Completed",
      "dot": {
        "fill": "#3d8d3d"
      },
      "stroke": "#3d8d3d"
    },
    {
      "key": "StoryPlanned.ToDo",
      "dot": {
        "fill": "#ded739"
      },
      "stroke": "#ded739"
    },
    {
      "key": "Defects.Backlog",
      "strokeDasharray": "5 5",
      "dot": {
        "fill": "#de5e3f"
      },
      "stroke": "#de5e3f"
    },
    {
      "key": "Defects.Completed",
      "strokeDasharray": "5 5",
      "dot": {
        "fill": "#ffb200"
      },
      "stroke": "#ffb200"
    },
    {
      "key": "Defects.ToDo",
      "strokeDasharray": "5 5",
      "dot": {
        "fill": "#e00e2c"
      },
      "stroke": "#e00e2c"
    }
  ]
}